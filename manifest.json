{
  "manifest_version": 3,
  "name": "Cookie 获取助手",
  "version": "1.0",
  "description": "一键复制当前网站 Cookie",
  "permissions": ["cookies", "activeTab", "scripting", "clipboardWrite"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}